@inproceedings{daskalakis_strong_2015,
	address = {New York, New York, USA},
	title = {Strong {Duality} for a {Multiple}-{Good} {Monopolist}},
	isbn = {978-1-4503-3410-5},
	url = {http://dl.acm.org/citation.cfm?doid=2764468.2764539},
	doi = {10.1145/2764468.2764539},
	abstract = {We characterize optimal mechanisms for the multiple-good monopoly problem and provide a framework to find them. We show that a mechanism is optimal if and only if a measure μ derived from the buyer's type distribution satisfies certain stochastic dominance conditions. This measure expresses the marginal change in the seller's revenue under marginal changes in the rent paid to subsets of buyer types. As a corollary, we characterize the optimality of grand-bundling mechanisms, strengthening several results in the literature, where only sufficient optimality conditions have been derived. As an application, we show that the optimal mechanism for \$n\$ independent uniform items each supported on \$[c,c+1]\$ is a grand-bundling mechanism, as long as \$c\$ is sufficiently large, extending Pavlov's result for \$2\$ items [Pavlov'11]. At the same time, our characterization also implies that, for all \$c\$ and for all sufficiently large \$n\$, the optimal mechanism for \$n\$ independent uniform items supported on \$[c,c+1]\$ is not a grand bundling mechanism.},
	booktitle = {Proceedings of the {Sixteenth} {ACM} {Conference} on {Economics} and {Computation} - {EC} '15},
	publisher = {ACM Press},
	author = {Daskalakis, Constantinos and Deckelbaum, Alan and Tzamos, Christos},
	year = {2015},
	note = {\_eprint: arXiv:1409.4150v3},
	keywords = {a microsoft research faculty, and ccf-1101491, and nsf, awards ccf-0953960, career, fellowship, foundation fellowship, grand bundling, mechanism design, revenue maximization, strong duality, supported by a sloan},
	pages = {449--450},
}

@article{chiappori_multidimensional_2016,
	title = {Multidimensional matching},
	url = {http://arxiv.org/abs/1604.05771},
	abstract = {We present a general analysis of multidimensional matching problems with transferable utility, paying particular attention to the case in which the dimensions of heterogeneity on the two sides of the market are unequal. A particular emphasis is put on problems where agents on one side of the market are multidimensional and agents on the other side are uni-dimensional, we describe a general approach to solve such problems. Lastly, we analyze several examples, including an hedonic model with differentiated products, a marriage market model where wives are differentiated in income and fertility, and a competitive variation of the Rochet-Choné problem. In the latter example, we show that the bunching phenomena, observed by Rochet and Choné in the monopoly context, do not occur in the competitive context},
